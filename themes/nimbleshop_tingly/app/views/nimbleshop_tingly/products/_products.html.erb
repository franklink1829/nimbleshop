<div class='span10'>

  <%= render partial: 'nimbleshop_tingly/products/breadcrumb' %>

  <% products.in_groups_of(3) do |group| %>
     <ul class="unstyled row">
        <% group.each do |product| %>
            <% if product.blank? %>
              &nbsp;
            <% else %>
              <li class="span3">
                <%= render 'nimbleshop_tingly/products/product', product: product %>
              </li>
            <% end %>
        <% end %>
     </ul>
  <% end %>
</div>
